<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatis.quartz.db.dao.JobDefineDbDao">
    <resultMap id="BaseResultMap" type="com.example.mybatis.quartz.db.JobDefineDb">
        <id column="id" property="id"/>
        <result property="name" column="name"/>
        <result property="jobFlowId" column="job_flow_id"/>
        <result property="sn" column="sn"/>
        <result property="status" column="status"/>
        <result property="cron" column="cron"/>
        <result property="created" column="created"/>
        <result property="deleted" column="deleted"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, `name`, job_flow_id, sn, status, cron, created, deleted
    </sql>

    <select id="selectBySelective" resultMap="BaseResultMap" parameterType="com.example.mybatis.quartz.db.JobDefineDb">
        select <include refid="Base_Column_List"/>
        from job_define
        <where>
            <if test="id != null">
                id = #{id}
            </if>
            <if test="name != null">
                and `name` like '%#{name}%'
            </if>
            <if test="jobFlowId != null">
                and job_flow_id = #{jobFlowId}
            </if>
            <if test="sn != null">
                and sn = #{sn}
            </if>
            <if test="status != null">
                and status = #{status}
            </if>
            <if test="deleted != null">
                and deleted = #{deleted}
            </if>
        </where>
    </select>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="Long">
        select <include refid="Base_Column_List"/>
        from job_define
        where id = #{id}
    </select>

    <insert id="insert" parameterType="com.example.mybatis.quartz.db.JobDefineDb" keyProperty="id" useGeneratedKeys="true">
        INSERT into job_define(id, `name`, job_flow_id, sn, status, cron, created, deleted)
        VALUES (#{id}, #{name}, #{jobFlowId}, #{sn}, #{status}, #{cron}, #{created}, #{deleted})
    </insert>

    <update id="updateByPrimaryKeySelective" parameterType="com.example.mybatis.quartz.db.JobDefineDb">
        update job_define
        <set>
            <if test="name != null">
                `name` = #{name},
            </if>
            <if test="jobFlowId != null">
                job_flow_id = #{jobFlowId},
            </if>
            <if test="sn != null">
                sn = #{sn},
            </if>
            <if test="status != null">
                status = #{status},
            </if>
            <if test="cron != null">
                cron = #{cron},
            </if>
            <if test="deleted != null">
                deleted = #{deleted}
            </if>
        </set>
        WHERE  id = #{id}
    </update>

</mapper>